{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm run install:all",
    "rewrites": [
        {
            "source": "/teacher",
            "destination": "/teacher/index.html"
        },
        {
            "source": "/teacher/:path*",
            "destination": "/teacher/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}