{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm run install:all",
    "rewrites": [
        {
            "source": "/teacher/:path*",
            "destination": "/teacher_dashboard/:path*"
        },
        {
            "source": "/admin/:path*",
            "destination": "/admin_dashboard/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/admin_dashboard"
        }
    ]
}