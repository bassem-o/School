{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm run install:all",
    "rewrites": [
        {
            "source": "/teacher/:path*",
            "destination": "/teacher/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/:path*"
        }
    ]
}